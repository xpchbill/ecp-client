{
  "name": "ecp-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "cp deploy/.env_dev .env && vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "echo \"Everything gonna be okay\"",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "ant-design-vue": "^1.4.10",
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "register-service-worker": "^1.6.2",
    "uid": "0.0.2",
    "vue": "^2.6.10",
    "vue-croppie": "^2.0.1",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1",
    "vuex-cache": "^3.1.0",
    "vuex-persist": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-pwa": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "prettier": "^1.18.2",
    "vue-ls": "^3.2.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-theme-color-replacer": "^1.3.3",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "vue/html-quotes": "warning",
      "vue/html-self-closing": "warning",
      "vue/no-multi-spaces": "warning",
      "vue/require-prop-types": "warning",
      "vue/no-confusing-v-for-v-if": "warning",
      "vue/no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "/public/",
    "/dist/",
    "/build/",
    "/docs/",
    "/node_modules/"
  ],
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "jsxBracketSameLine": true
  },
  "prettierIgnore": [
    "build/",
    "/dist/",
    "node_modules/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpchbill/ecp-client.git"
  },
  "keywords": [],
  "author": "Bill Xiong <xpchbill8@gmail.com>",
  "bugs": {
    "url": "https://github.com/xpchbill/ecp-client/issues"
  },
  "homepage": "https://github.com/xpchbill/ecp-client#readme"
}
